                <mxCell id="bLNiGClRiuzVImFWI04B-4" value="&lt;font color=&quot;#ffffff&quot;&gt;提供信息&lt;/font&gt;" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];fontColor=#000000;" vertex="1" connectable="0" parent="bLNiGClRiuzVImFWI04B-3">
